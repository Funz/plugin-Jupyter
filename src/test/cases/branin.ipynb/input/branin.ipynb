   "source": [
    "import math as m\n",
    "def branin(x1,x2):\n",
    "    x1 = x1*15-5\n",
    "    x2 = x2*15\n",
    "\n",
    "    return (x2 - 5/(4*m.pi**2)*(x1**2) + 5/m.pi*x1 -6 )**2 + 10*(1-1/(8*m.pi))*m.cos(x1) +10"
   ]
  },
  {
   "cell_type": "raw",
   "metadata": {},
   "source": [
    "then, eval branin:"
   ]
  },
